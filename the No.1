#include<stdio.h>
#include<string.h> 
 
 
 int main ()
 {
 	int N=0,t=0,sum=0,a=0;
	int qm[100]={0},py[100]={0},num[100]={0},jiangjing[100]={0};
 	char xsgb[100],xbsf[100];
 	char name[100][50];
 		scanf("%d",&N);
 	for(int i=0;i<N;i++){
 		scanf("%s %d %d %c %c %d\n",&name[i],&qm[i],&py[i],&xsgb[i],&xbsf[i],&num[i]);
		if(qm[i]>80&&num[i]>0){
			jiangjing[i]+=8000;
		} 
		if(qm[i]>85&&py[i]>80){
			jiangjing[i]+=4000;
		}
		if(qm[i]>90){
			jiangjing[i]+=2000;
		}
		if(qm[i]>85&&xbsf[i]=='Y'){
			jiangjing[i]+=1000;
		}
		if(py[i]>80&&xsgb[i]=='Y'){
			jiangjing[i]+=850;
		}
	} 
	for(int j=0;j<N;j++){
	    if(jiangjing[j]>t){
	    	t=jiangjing[j];
	    	a=j;
		}
		sum+=jiangjing[j];
	}
	printf("%s %d %d\n",name[a],t,sum);
 	return 0;
 }
